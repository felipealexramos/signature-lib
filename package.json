{
  "name": "@felipealexandre/signature-lib",
  "version": "1.1.0",
  "description": "A pluggable signature library with support for canvas, Topaz devices, and more.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist"
  },
  "keywords": ["signature", "canvas", "topaz", "digital-signature", "adapter"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "tsup": "^8.0.0"
  }
}